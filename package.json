{
  "name": "messages-by-twiage-prototype",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --ext .js,.vue",
    "major": "npm version major -m 'Build v%s' && git push --follow-tags",
    "minor": "npm version minor -m 'Build v%s' && git push --follow-tags",
    "patch": "npm version patch -m 'Build v%s' && git push --follow-tags",
    "premajor": "npm run type-check_format-check_lint",
    "preminor": "npm run type-check_format-check_lint",
    "prepatch": "npm run type-check_format-check_lint",
    "serve": "vite preview",
    "start": "npm run dev"
  },
  "dependencies": {
    "@popperjs/core": "^2.7.0",
    "@urql/exchange-auth": "^0.1.3",
    "@urql/vue": "^0.3.0",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.3.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "jwt-decode": "^3.1.2",
    "subscriptions-transport-ws": "^0.9.18",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.6.0",
    "prettier": "^2.2.1",
    "vite": "^2.0.0"
  }
}
